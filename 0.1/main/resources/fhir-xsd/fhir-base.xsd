						maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Establishes the namespace in which set of
								possible id values is unique.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="value" type="string" minOccurs="0"
						maxOccurs="1">
						<xs:annotation>
